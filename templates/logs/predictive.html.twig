

<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %} Search logs {% endblock %}</title>
        <link rel="icon" href="https://img.icons8.com/nolan/64/permanent-job.png">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <link rel="stylesheet" href="{{ asset('assets/css/_bootswatch.scss') }}">
            <link rel="stylesheet" href="{{ asset('assets/css/_variables.scss') }}">
            <link rel="stylesheet" href="{{ asset('https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css') }}">
             <link rel="stylesheet" href={{asset('assets/css/index.css')}}>
           
        {% endblock %}

    </head>
    <body class="main layout">
{% block head %}
    
{% endblock %}
{% block body %}


<div class="container mt-4">
    <div class="row">
            <div class='col-2'>
                type
            </div>
             <div class='col-4'>
                 elastic query
            </div>
             <div class='col-4'>
                  response
            </div>
             <div class='col-2'>
                  date
            </div>
       </div>
    {% for log in logs %}
       <div class="row mt-4">
            <div class='col-2'>
                {{log.type}}
            </div>
             <div class='col-4'>
                 <pre>{{ log.request}}</pre>
            </div>
             <div class='col-4'>
                  <pre>{{log.response}} </pre>
            </div>
             <div class='col-2'>
                  {{log.date|date('m/d/Y H:i:s')}}
            </div>
       </div>
    {% endfor %}
</div>

{% endblock %}